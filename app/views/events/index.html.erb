<h1>All Events</h1>

<ul>
  <% @events.each do |event| %>
    <li>
      <%= link_to event.title, event %>
    </li>
  <% end %>
</ul>

<% if user_signed_in?  %>
  <%= link_to "Sign Out", destroy_user_session_path, 'data-turbo-method': :delete %>
<% else %>
  <%= link_to "Sign in", new_user_session_path %>
<% end %>
